    <option Value=\"Nuez con chocolate\">Nuez con chocolate</option>\n        <option Value=\"Tradicional de chocolate\">Tradicional de chocolate</option>\n        <option Value=\"Zanahoria\">Zanahoria</option>\n        <option Value=\"Vainilla\">Vainilla</option>\n        <option Value=\"Alemán\">Alemán</option>\n        <option Value=\"Cajeta\">Cajeta</option>\n        <option Value=\"Chesscake de cereza\">Chesscake de cereza</option>\n        <option Value=\"Durazno\">Durazno</option>\n    </select>\n    <label for=\"DropDownCantidad\">Cantidad:</label>\n    <select required ID=\"DropDownCantidad\" v-model=\"pedido.cantidad\" Height=\"18px\" Width=\"187px\">\n        <option v-if=\"1<=restante\" Value=\"1\">1</option>\n        <option Value=\"2\">2</option>\n        <option Value=\"3\">3</option>\n        <option Value=\"4\">4</option>\n        <option Value=\"5\">5</option>\n        <option Value=\"6\">6</option>\n        <option Value=\"7\">7</option>\n        <option Value=\"8\">8</option>\n        <option Value=\"9\">9</option>\n        <option v-if=\"$value<=restante\" Value=\"10\">10</option>\n    </select>\n  </fieldset>\n </div>\n\n\n<!------------------------- armado de pastel----------------------------------->\n<div v-if=\"armarPastel\">\n\n<fieldset>\n    <legend>Bizcochos:</legend>\n      <input required type=\"radio\" id=\"Vainilla\" name=\"bizcocho\" v-model=\"pedido.bizcocho\" value=\"Vainilla\">\n      <label for=\"Vainilla\">Vainilla</label><br>\n      <input type=\"radio\" id=\"Chocolate\" name=\"bizcocho\" v-model=\"pedido.bizcocho\" value=\"Chocolate\">\n      <label for=\"Chocolate\">Chocolate</label><br>\n      <input type=\"radio\" id=\"Marmoleado\" name=\"bizcocho\" v-model=\"pedido.bizcocho\" value=\"Marmoleado\">\n      <label for=\"Marmoleado\">Marmoleado</label><br>\n      <input type=\"radio\" id=\"Naranja\" name=\"bizcocho\" v-model=\"pedido.bizcocho\" value=\"Naranja\">\n      <label for=\"Naranja\">Naranja</label><br>\n      <input type=\"radio\" id=\"Red Velvet\" name=\"bizcocho\" v-model=\"pedido.bizcocho\" value=\"Red Velvet\">\n      <label for=\"Red Velvet\">Red Velvet</label><br>\n      <input type=\"radio\" id=\"Almendra\" name=\"bizcocho\" v-model=\"pedido.bizcocho\" value=\"Almendra\">\n      <label for=\"Almendra\">Almendra</label><br>\n      <input type=\"radio\" id=\"Limon\" name=\"bizcocho\" v-model=\"pedido.bizcocho\" value=\"Limon\">\n      <label for=\"Limon\">Limón</label><br>\n  </fieldset>\n  <fieldset>\n    <legend>Rellenos:</legend>\n    <label for=\"relleno\">Elige el relleno:</label>\n    <select name=\"relleno\" v-model=\"pedido.relleno\" id=\"relleno\">\n      <option disabled value=\"\">Seleccione un relleno</option>\n      <optgroup label=\"Mermeladas\">\n        <option value=\"Mermelada de Fresa\" >Fresa</option>\n        <option value=\"Mermelada de Durazno\">Durazno</option>\n        <option value=\"Mermelada de Guayaba\">Guayaba</option>\n        <option value=\"Mermelada de Frambuesa\">Frambuesa</option>\n      </optgroup>\n      <optgroup label=\"Cremas\">\n        <option value=\"Crema de Fresa\" >Fresa</option>\n        <option value=\"Crema de Durazno\">Durazno</option>\n        <option value=\"Crema de Café\">Café</option>\n        <option value=\"Crema de Vainilla\">Vainilla</option>\n      </optgroup>\n    </select>    \n  </fieldset>\n  <fieldset>\n    <legend>Coberturas y toppings:</legend>\n    <label for=\"cobertura\">Elige la cobertura:</label>\n    <select name=\"cobertura\" id=\"cobertura\" v-model=\"pedido.cobertura\">\n        <option value=\"Betún de nata\" >Betún de nata</option>\n        <option value=\"Betún de chocolate\">Betún de chocolate</option>\n        <option value=\"Betún de vainilla\">Betún de vainilla</option>\n        <option value=\"Buttercream\">Buttercream</option>\n        <option value=\"Queso crema\">Queso crema</option>\n    </select> \n    <br>\n    <br>\n    <label >Elige los toppings:</label><br>\n    <input type=\"checkbox\" id=\"Brownies\" name=\"Brownies\" v-model=\"pedido.toppings\" value=\"Brownies\">\n    <label for=\"Brownies\"> Brownies</label><br>\n    <input type=\"checkbox\" id=\"cobertura_pastel\" name=\"cobertura_pastel\" v-model=\"pedido.toppings\" value=\"Pastel Imposible\">\n    <label for=\"cobertura_pastel\">cobertura pastel imposible</label><br>\n    <input type=\"checkbox\" id=\"Fruta\" name=\"Fruta\" v-model=\"pedido.toppings\" value=\"Fruta\">\n    <label for=\"Fruta\">Fruta de temporada</label><br>\n    <input type=\"checkbox\" id=\"Ganage\" name=\"Ganage\" v-model=\"pedido.toppings\" value=\"Ganage\">\n    <label for=\"Ganage\"> Ganage</label><br>\n    <input type=\"checkbox\" id=\"Nutella\" name=\"Nutella\" v-model=\"pedido.toppings\" value=\"Nutella\">\n    <label for=\"Nutella\"> Nutella</label><br>\n    <input type=\"checkbox\" id=\"CreamCarlota\" name=\"CreamCarlota\" v-model=\"pedido.toppings\" value=\"Cream Carlota\">\n    <label for=\"CreamCarlota\"> Cream Carlota</label><br>\n  </fieldset>\n</div>\n\n\n  \n  <br>\n  <!-- <input  type=\"submit\" style=\" height: 30px;\"  value=\"Enviar\"> -->\n\n  </form>\n  <button @click=\"guardarPedido()\">Realizar pedido</button>\n</div>\n\n</template>\n\n<script>\nexport default {\n  name: 'FormPasteles',\n  props: {armarPastel:Boolean},\n\n  data() {\n            // armar: this.$route.params.armarPastel,\n       return{pedido:{\n              nombre: '',\n              comentarios: '',\n              email: '',\n              telefono: '',\n              sabores: '',\n              cantidad: 1,\n              bizcocho: '',\n              relleno: '',\n              cobertura:'',\n              toppings: [],\n              armar:this.armarPastel\n            },\n             restante:0\n            }\n    },\n  methods: {\n    pikachu(){\n        console.log(this.pedido);\n\n\n\n    },\n    guardarPedido(){\n       \n\n      this.$store.commit(\"guardarPedido\",this.pedido);\n     \n\n      if(!this.armarPastel){\n         const tipoPastel = this.pedido.sabores;\n         const cantidad = +this.pedido.cantidad;\n        this.$store.commit('actualizarCantidad',{tipoPastel,cantidad})\n      }\n      this.$refs.form.reset();\n      this.$router.push({ path: '/' });\n    },\n    validarRestante(tipoPastel){\n    \n\n         switch (tipoPastel){\n        case 'Milky way':\n          this.restante =this.$store.state.mk;   \n          break;\n        case 'Moka':\n          this.restante =this.$store.state.moka;   \n          break;  \n        case 'Nuez con chocolate':\n          this.restante =this.$store.state.nc;   \n          break;  \n        case 'Tradicional de chocolate':\n          this.restante =this.$store.state.tc;   \n          break;\n        case 'Zanahoria':\n          this.restante =this.$store.state.zanahoria;   \n          break;  \n        case 'Vainilla':\n          this.restante =this.$store.state.vainilla;   \n          break;\n        case 'Alemán':\n          this.restante =this.$store.state.aleman;   \n          break;\n        case 'Cajeta':\n          this.restante =this.$store.state.cajeta;   \n          break;    \n        case 'Chesscake de cereza':\n          this.restante =this.$store.state.cc;   \n          break;   \n        case 'Durazno':\n          this.restante =this.$store.state.durazno;   \n          break;           \n        default:\n          this.restante =10;   \n      }\n    }\n  },\n  created(){\n    this.validarRestante();\n  }\n\n   \n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}