{"ast":null,"code":"export default {\n  name: 'FormPasteles',\n  props: {\n    armarPastel: Boolean\n  },\n\n  data() {\n    // armar: this.$route.params.armarPastel,\n    return {\n      pedido: {\n        nombre: '',\n        comentarios: '',\n        email: '',\n        telefono: '',\n        sabores: '',\n        cantidad: 1,\n        bizcocho: '',\n        relleno: '',\n        cobertura: '',\n        toppings: [],\n        armar: this.armarPastel\n      },\n      restante: 0\n    };\n  },\n\n  methods: {\n    pikachu() {\n      console.log(this.pedido);\n    },\n\n    guardarPedido() {\n      this.$store.commit(\"guardarPedido\", this.pedido);\n\n      if (!this.armarPastel) {\n        const tipoPastel = this.pedido.sabores;\n        const cantidad = +this.pedido.cantidad;\n        this.$store.commit('actualizarCantidad', {\n          tipoPastel,\n          cantidad\n        });\n      }\n\n      this.$refs.form.reset();\n      this.$router.push({\n        path: '/'\n      });\n    },\n\n    validarRestante(tipoPastel) {\n      switch (tipoPastel) {\n        case 'Milky way':\n          this.restante = this.$store.state.mk;\n          break;\n\n        case 'Moka':\n          this.restante = this.$store.state.moka;\n          break;\n\n        case 'Nuez con chocolate':\n          this.restante = this.$store.state.nc;\n          break;\n\n        case 'Tradicional de chocolate':\n          this.restante = this.$store.state.tc;\n          break;\n\n        case 'Zanahoria':\n          this.restante = this.$store.state.zanahoria;\n          break;\n\n        case 'Vainilla':\n          this.restante = this.$store.state.vainilla;\n          break;\n\n        case 'Alemán':\n          this.restante = this.$store.state.aleman;\n          break;\n\n        case 'Cajeta':\n          this.restante = this.$store.state.cajeta;\n          break;\n\n        case 'Chesscake de cereza':\n          this.restante = this.$store.state.cc;\n          break;\n\n        case 'Durazno':\n          this.restante = this.$store.state.durazno;\n          break;\n\n        default:\n          this.restante = 10;\n      }\n    }\n\n  },\n\n  created() {\n    this.validarRestante();\n  }\n\n};","map":{"version":3,"mappings":"AAyIA,eAAe;AACbA,MAAI,EAAE,cADO;AAEbC,OAAK,EAAE;AAACC,eAAW,EAACC;AAAb,GAFM;;AAIbC,MAAI,GAAG;AACG;AACL,WAAM;AAACC,YAAM,EAAC;AACPC,cAAM,EAAE,EADD;AAEPC,mBAAW,EAAE,EAFN;AAGPC,aAAK,EAAE,EAHA;AAIPC,gBAAQ,EAAE,EAJH;AAKPC,eAAO,EAAE,EALF;AAMPC,gBAAQ,EAAE,CANH;AAOPC,gBAAQ,EAAE,EAPH;AAQPC,eAAO,EAAE,EARF;AASPC,iBAAS,EAAC,EATH;AAUPC,gBAAQ,EAAE,EAVH;AAWPC,aAAK,EAAC,KAAKd;AAXJ,OAAR;AAaAe,cAAQ,EAAC;AAbT,KAAN;AAeF,GArBU;;AAsBbC,SAAO,EAAE;AACPC,WAAO,GAAE;AACLC,aAAO,CAACC,GAAR,CAAY,KAAKhB,MAAjB;AAIH,KANM;;AAOPiB,iBAAa,GAAE;AAGb,WAAKC,MAAL,CAAYC,MAAZ,CAAmB,eAAnB,EAAmC,KAAKnB,MAAxC;;AAGA,UAAG,CAAC,KAAKH,WAAT,EAAqB;AAClB,cAAMuB,UAAS,GAAI,KAAKpB,MAAL,CAAYK,OAA/B;AACA,cAAMC,QAAO,GAAI,CAAC,KAAKN,MAAL,CAAYM,QAA9B;AACD,aAAKY,MAAL,CAAYC,MAAZ,CAAmB,oBAAnB,EAAwC;AAACC,oBAAD;AAAYd;AAAZ,SAAxC;AACF;;AACA,WAAKe,KAAL,CAAWC,IAAX,CAAgBC,KAAhB;AACA,WAAKC,OAAL,CAAaC,IAAb,CAAkB;AAAEC,YAAI,EAAE;AAAR,OAAlB;AACD,KApBM;;AAqBPC,mBAAe,CAACP,UAAD,EAAY;AAGtB,cAAQA,UAAR;AACD,aAAK,WAAL;AACE,eAAKR,QAAL,GAAe,KAAKM,MAAL,CAAYU,KAAZ,CAAkBC,EAAjC;AACA;;AACF,aAAK,MAAL;AACE,eAAKjB,QAAL,GAAe,KAAKM,MAAL,CAAYU,KAAZ,CAAkBE,IAAjC;AACA;;AACF,aAAK,oBAAL;AACE,eAAKlB,QAAL,GAAe,KAAKM,MAAL,CAAYU,KAAZ,CAAkBG,EAAjC;AACA;;AACF,aAAK,0BAAL;AACE,eAAKnB,QAAL,GAAe,KAAKM,MAAL,CAAYU,KAAZ,CAAkBI,EAAjC;AACA;;AACF,aAAK,WAAL;AACE,eAAKpB,QAAL,GAAe,KAAKM,MAAL,CAAYU,KAAZ,CAAkBK,SAAjC;AACA;;AACF,aAAK,UAAL;AACE,eAAKrB,QAAL,GAAe,KAAKM,MAAL,CAAYU,KAAZ,CAAkBM,QAAjC;AACA;;AACF,aAAK,QAAL;AACE,eAAKtB,QAAL,GAAe,KAAKM,MAAL,CAAYU,KAAZ,CAAkBO,MAAjC;AACA;;AACF,aAAK,QAAL;AACE,eAAKvB,QAAL,GAAe,KAAKM,MAAL,CAAYU,KAAZ,CAAkBQ,MAAjC;AACA;;AACF,aAAK,qBAAL;AACE,eAAKxB,QAAL,GAAe,KAAKM,MAAL,CAAYU,KAAZ,CAAkBS,EAAjC;AACA;;AACF,aAAK,SAAL;AACE,eAAKzB,QAAL,GAAe,KAAKM,MAAL,CAAYU,KAAZ,CAAkBU,OAAjC;AACA;;AACF;AACE,eAAK1B,QAAL,GAAe,EAAf;AAhCD;A